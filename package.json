  "scripts": {
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "todo-test": "./node_modules/.bin/gulp tests",
    "todo-watch": "./node_modules/.bin/gulp watch",
    "todo-prebuild": "gulp increment-buildnum",
    "todo-build": "./scripts/build.sh",
    "todo-postbuild": "./scripts/build-deb.sh",
    "todo-build-no-bump": "./scripts/build.sh",
    "start": "npm run build && npm run start-no-build",
    "clean": "rm -rf .cache dist",
    "watch": "parcel --target=electron src/background/entry.ts src/sandbox/entry.tsx src/ui/entry.js",
    "build": "parcel build --target=electron --public-url '.' src/background/entry.ts src/sandbox/entry.tsx src/ui/entry.js",
    "start-no-build": "ANSEL_DEV_MODE=true electron ."
  },
