{
  "name": "ansel",
  "productName": "Ansel",
  "version": "1.0.0",
  "description": "Digital image organizer powered by the web",
  "author": "Lo√Øc Nogues <nogues.loic@gmail.com>, Til Schneider <github@murfman.de>",
  "license": "MIT",
  "main": "dist/background.js",
  "engineStrict": true,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "build": {
    "appId": "github.ansel",
    "mac": {
      "category": "public.app-category.photography",
      "electronLanguages": [ "de", "en" ]
    },
    "linux": {
      "category": "Graphics",
      "asar": false
    },
    "publish": null,
    "directories": {
      "app": ".",
      "output": "dist-package",
      "buildResources": "src/package"
    },
    "files": [
      "dist/**",
      "!**/node_modules/sharp/docs",
      "!**/node_modules/sharp/src",
      "!**/node_modules/sqlite3/src"
    ],
    "extraResources": {
      "to": "app",
      "filter": [
        "menus/**",
        "migrations/**"
      ]
    }
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "clean": "rm -rf dist dist-package",
    "clean-package": "rm -rf dist-package",
    "start": "yarn run build && yarn run start-no-build",
    "watch": "NODE_ENV=development webpack -w",
    "build": "NODE_ENV=development webpack",
    "build-production": "NODE_ENV=production webpack",
    "start-no-build": "ANSEL_DEV_MODE=true electron .",
    "package-dir": "electron-builder --dir",
    "package": "electron-builder",
    "release": "yarn run clean && yarn run build-production && yarn run package",
    "release:windows": "yarn run clean && set \"NODE_ENV=production\" && webpack && yarn run package"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.18.0",
    "@blueprintjs/select": "^3.10.0",
    "bluebird": "^3.5.5",
    "chokidar": "3.0.2",
    "classnames": "^2.2.6",
    "exif-parser": "^0.1.12",
    "font-awesome": "^4.7.0",
    "fs.extra": "^1.3.2",
    "gl-matrix": "^3.0.0",
    "json-stringify-pretty-compact": "^2.0.0",
    "justified-layout": "^3.0.0",
    "moment": "^2.24.0",
    "node-notifier": "5.4.0",
    "node-pre-gyp": "^0.13.0",
    "normalize.css": "^8.0.1",
    "pretty-error": "^2.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.4",
    "redux-logger": "3.0.6",
    "redux-thunk": "^2.3.0",
    "sharp": "0.23.0",
    "source-map-support": "^0.5.13",
    "sqlite3": "^4.0.9",
    "sqlite3-helper": "1.2.1",
    "typesafe-actions": "^4.4.2"
  },
  "optionalDependencies": {
    "libraw": "file:submodules/node-libraw"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/classnames": "^2.2.9",
    "@types/gl-matrix": "^2.4.5",
    "@types/node": "~12.0.0",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/sharp": "^0.22.2",
    "@types/sqlite3": "^3.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.1.0",
    "electron": "5.0.8",
    "electron-builder": "^21.2.0",
    "file-loader": "^4.1.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-gyp": "^5.0.3",
    "style-loader": "^0.23.1",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-fix-default-import-plugin": "^1.0.3",
    "webpack-node-externals": "^1.7.2"
  },
  "repository": "https://github.com/ansel-app/ansel.git"
}
